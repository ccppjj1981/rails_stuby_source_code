# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal
/db/redis
/db/redis/**/*.rdb

# Ignore all logfiles and tempfiles.
/log
/tmp
/trash

# db
/backup

# Ignore static files
/public/assets
/public/uploads
/public/sites
/public/themes
/public/download

# Ignore sconfig
/config/database.yml
/config/secrets.yml
/config/settings.yml
/config/redis.yml
/config/redis.conf


# ssl
/ssl
